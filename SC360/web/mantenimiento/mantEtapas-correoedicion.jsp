<!--REGISTRO CORREO-->
<form role="form">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title">Mantenimiento del Correo</h3>
        </div>
        <div class="panel-body">

            <div class="container" style="width: 700px;">
                <div class="row">
                    <div class="form-group">
                        <label class="control-label">Nombre</label>
                        <input
                            maxlength="100"
                            type="text"
                            required="required"
                            class="form-control"
                            placeholder="Enter First Name"
                            />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Correo</label>
                        <input
                            maxlength="100"
                            type="text"
                            required="required"
                            class="form-control"
                            placeholder="Enter Last Name"
                            />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Supervisor</label>
                        <input
                            maxlength="100"
                            type="text"
                            required="required"
                            class="form-control"
                            placeholder="Enter Last Name"
                            />
                    </div>
                    <div class="form-group">
                        <label class="control-label">Jefe</label>
                        <input
                            maxlength="100"
                            type="text"
                            required="required"
                            class="form-control"
                            placeholder="Enter Last Name"
                            />
                    </div>
                </div>
            </div>

        </div>
    </div>
    <button
        class="btn btn-primary nextBtn pull-right"
        type="button"
        >
        Grabar
    </button>
</form>