<form role="form">
    <div
        class="panel panel-primary setup-content"
        
        style="width: 700px;"
        >
        <div class="panel-heading">
            <h3 class="panel-title">Ingreso de Tramite</h3>
        </div>
        <div class="panel-body"  style="width: auto;height: 340px;overflow-y: auto;">
            <div class="form-group">
                <label class="control-label">Tramite</label>
                <select
                    maxlength="100"                    
                    required="required"
                    class="form-control"

                    ><option>Selecciona</option></select>
            </div>
            <div class="form-group">
                <label class="control-label">Tipo</label>
                <select
                    maxlength="100"                    
                    required="required"
                    class="form-control"

                    ><option>Selecciona</option></select>
            </div>
            <div class="form-group">
                <label class="control-label">Etapa</label>
                <select
                    maxlength="100"                    
                    required="required"
                    class="form-control"

                    ><option>Selecciona</option></select>
            </div>
            <div class="form-group">
                <label class="control-label">Plazo</label>
                <input
                    maxlength="100"
                    type="text"
                    required="required"
                    class="form-control"
                    placeholder="Enter Last Name"
                    />
            </div>
            <div class="form-group">
                <label class="control-label">Tiempo</label>
                <input
                    maxlength="100"
                    type="text"
                    required="required"
                    class="form-control"
                    placeholder="Enter Last Name"
                    />
            </div>
            <button
                class="btn btn-primary nextBtn pull-right"
                type="button"
                >
                Next
            </button>
        </div>
    </div>
</form>